{
    "manifest_version": 3,
    "name": "GO BACK TO WORK",
    "version": "1.0",
    "description": "Working you must.",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "public/favicon-16x16.png",
            "48": "public/favicon-32x32.png",
            "128": "public/android-chrome-192x192.png"
        }
    },
    "icons": {
        "16": "public/favicon-16x16.png",
        "48": "public/favicon-32x32.png",
        "128": "public/android-chrome-512x512.png"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}
